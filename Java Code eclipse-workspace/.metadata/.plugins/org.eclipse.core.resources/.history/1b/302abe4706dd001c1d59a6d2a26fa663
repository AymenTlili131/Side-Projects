package TP2_ex2;

public class ThreadDeposer extends Thread{
	double solde;
	Compte_ x;
	public ThreadDeposer(double solde,Compte_ c) {
		this.solde = solde;
	}
	
	public void Déposer(double montant) 
	{	
		try {
			this.x.déposer(montant);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		}
	
}
